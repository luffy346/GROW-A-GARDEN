-- Grow A Garden Script UI
-- Made for Delta Executor

-- Create the ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "GrowAGardenScript"
screenGui.ResetOnSpawn = false
screenGui.IgnoreGuiInset = true
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Create the black background
local background = Instance.new("Frame")
background.Size = UDim2.new(1, 0, 1, 0)
background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
background.Parent = screenGui

-- Title Text
local title = Instance.new("TextLabel")
title.Text = "Grow A Garden Script"
title.Size = UDim2.new(1, 0, 0, 100)
title.Position = UDim2.new(0, 0, 0.3, 0)
title.TextColor3 = Color3.fromRGB(255, 100, 100)
title.TextScaled = true
title.Font = Enum.Font.FredokaOne
title.BackgroundTransparency = 1
title.Parent = background

-- Warning Text
local warning = Instance.new("TextLabel")
warning.Text = "IMPORTANT: If u leave now u will get banned as the\nscript is currently interfering with the anti cheat"
warning.Size = UDim2.new(1, -100, 0, 200)
warning.Position = UDim2.new(0.5, -500/2, 0.4, 0)
warning.TextColor3 = Color3.fromRGB(170, 150, 255)
warning.TextScaled = true
warning.Font = Enum.Font.FredokaOne
warning.TextWrapped = true
warning.BackgroundTransparency = 1
warning.Parent = background

-- Equipment Buttons
local items = {
    "Trowel\n[186x Uses]",
    "Shovel\n[Destroy Plants]",
    "Master Sprinkler\nx1",
    "Basic Sprinkler\nx355",
    "Advanced Sprinkler\nx73"
}

for i, itemName in ipairs(items) do
    local item = Instance.new("TextButton")
    item.Text = itemName
    item.Size = UDim2.new(0, 150, 0, 50)
    item.Position = UDim2.new(0, 200 * (i-1) + 50, 0.8, 0)
    item.TextColor3 = Color3.fromRGB(255, 255, 255)
    item.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    item.Font = Enum.Font.FredokaOne
    item.TextScaled = true
    item.Parent = background
end-- Grow A Garden Script UI
